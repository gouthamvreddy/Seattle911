  "description": "Map and data of Seattle 911 incidents",
  "main": "server.js",
  "scripts": {
    "start": "babel-node tools/devServer.js & node server.js & mongod --dbpath ./db --smallfiles",
    "clean-dist": "npm run remove-dist && mkdir dist",
